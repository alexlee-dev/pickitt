{
  "name": "pickitt",
  "version": "3.2.0",
  "description": "When you need a computer to just pick it, reach for Pickitt!",
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src/**/*",
    "build": "npm run clean && npm run lint && tsc --declaration",
    "test": "jest",
    "coverage": "jest --coverage",
    "build-docs": "rimraf docs && typedoc --out docs src --mode modules --includeDeclarations --excludeExternals --name 'Pickitt' --includeVersion"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlee-dev/pickitt.git"
  },
  "keywords": [
    "random",
    "list",
    "picker",
    "pick",
    "choose",
    "chooser",
    "utility",
    "utility-function",
    "sample"
  ],
  "author": {
    "name": "Alex Lee",
    "email": "alex@alexlee.dev",
    "url": "https://www.alexlee.dev/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexlee-dev/pickitt/issues"
  },
  "homepage": "https://pickitt.netlify.com/",
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/documentation/"
    ]
  },
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "figlet": "^1.3.0"
  }
}
